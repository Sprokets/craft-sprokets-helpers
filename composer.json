{
  "name": "sprokets/sprokets-helpers",
  "description": "Misc helpers",
  "type": "craft-plugin",
  "version": "3.1.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "sprokets-helpers"
  ],
  "support": {
    "docs": "https://github.com/craft-sprokets-helpers/sprokets-helpers/blob/master/README.md",
    "issues": "https://github.com/craft-sprokets-helpers/sprokets-helpers/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "sprokets",
      "homepage": "https://sprokets.net"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^4.0.0-alpha"
  },
  "autoload": {
    "psr-4": {
      "sprokets\\sproketshelpers\\": "src/"
    }
  },
  "extra": {
    "name": "sprokets-helpers",
    "handle": "sprokets-helpers",
    "hasCpSettings": true,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/craft-sprokets-helpers/sprokets-helpers/master/CHANGELOG.md",
    "class": "sprokets\\sproketshelpers\\Sproketshelpers"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": false,
      "craftcms/plugin-installer": false
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "craftcms/rector": "dev-main"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}